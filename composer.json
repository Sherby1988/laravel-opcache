{
    "name": "resido-fhm/laravel-opcache",
    "description": "PHP OPcache Artisan commands for Laravel.",
    "keywords": [
        "appstract",
        "opcache",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/resido-fhm/laravel-opcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Olav van Schie",
            "email": "mail@appstract.nl",
            "homepage": "https://appstract.nl",
            "role": "Developer"
        },
        {
            "name": "Sergei Shitikov",
            "email": "shitikov@ferienhausmiete.de",
            "homepage": "https://ferienhausmiete.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^8.3",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Appstract\\Opcache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appstract\\Opcache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appstract\\Opcache\\OpcacheServiceProvider"
            ]
        }
    }
}
